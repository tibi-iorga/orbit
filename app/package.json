{
  "name": "orbit",
  "version": "0.4.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio",
    "db:migrate:fts": "prisma db execute --file prisma/migrations/20250223000000_full_text_search/migration.sql --schema prisma/schema.prisma",
    "db:migrate:rls": "prisma db execute --file prisma/migrations/20260225000000_enable_rls/migration.sql --schema prisma/schema.prisma",
    "postinstall": "prisma generate",
    "precommit": "npm run lint && npm audit --audit-level=moderate",
    "version:patch": "npm version patch",
    "version:minor": "npm version minor",
    "version:major": "npm version major"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.9",
    "@heroicons/react": "^2.2.0",
    "@prisma/client": "^5.22.0",
    "bcryptjs": "^2.4.3",
    "date-fns": "^4.1.0",
    "next": "14.2.18",
    "next-auth": "^4.24.10",
    "openai": "^4.73.0",
    "papaparse": "^5.4.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^22.9.0",
    "@types/papaparse": "^5.3.14",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.47",
    "prisma": "^5.22.0",
    "tailwindcss": "^3.4.14",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3"
  }
}
